name: Second WF
on: push
jobs:
  second-job:
    name: Run on Ubuntu
    runs-on: ubuntu-latest
    steps:
      - name: Get code
        uses: actions/checkout@v3
      - name: Check maven version
        run: mv -v
  other-job:
    needs: second-job
    name: Run after second-job
    runs-on: ubuntu-latest
    steps:
      - name: Check maven version
        run: npm -v
        
